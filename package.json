{
  "name": "hietrack-server",
  "version": "0.0.1",
  "description": "A server for hietrack, a free-and-open-source issue tracker.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "npm run build && node dist/main.js",
    "lint:check": "npx eslint --ext .ts src/**",
    "format:check": "npx prettier --check src",
    "test": "jest",
    "validate": "npm run lint:check && npm run format:check && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninthapproach/hietrack-server.git"
  },
  "author": "Ninth Approach LLC <team@ninthapproach.com>",
  "contributors": [
    {
      "name": "Isaac Trimble-Pederson",
      "email": "isaac@ninthapproach.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ninthapproach/hietrack-server/issues"
  },
  "homepage": "https://github.com/ninthapproach/hietrack-server#readme",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  }
}
